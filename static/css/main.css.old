body {
    display: flex;
    padding: 2px 102px 0px 102px;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
    gap: 5px;
    background: darkslategrey; 
    color: white;
  }
  
  .top_bar {
    background-color: rgb(10, 36, 36);
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    border-radius: 15px;
    padding: 5px;
    align-items: center;
  }
  
  link_bar {
    display: flex;
    flex-direction: row;
  }
  
  table {
    border-collapse: collapse;
  }

  #uploadForm {
    width: 436px;
    height: 255.469px;
    border-radius: 6px;
    background: rgb(10, 36, 36) 50% / cover no-repeat;
    box-shadow: 0px 4px 44px 0px rgba(0, 0, 0, 0.17);
    padding: 20px;
    color: white;
  }
  
  button1 {
    margin: 5px;
    border-radius: 10px;
    background-color: rgb(10, 36, 36);
    padding: 5px;
  }


  input {
    padding: 5px;
    margin: 10px;
  }

  .q_bar {
    background-color: rgb(10, 36, 36);
    width: 50%;
    height: 40px;
    margin-top: 15px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    border-radius: 15px;
    padding: 5px;
    align-items: center;
  }
  
  a {
    color: white;
  }
  
  
  .tables{
    display: flex;
    flex-direction: row;
    padding: 10px;
    margin-top: 50px;
    gap:50px;
  }
  
  .mini{
    align-items: left;
    background-color: rgb(10, 36, 36);
    border-radius: 10px;
    padding: 15px;
    height: 300px;
    width: 400px;
    color: white;
  }
  
  .footer{
    width: 100%;
    background-color: darkblue;
    position:fixed;
    bottom: 0;
    background-color:  rgb(10, 36, 36);
    padding-left: 100px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    color: white;
  }